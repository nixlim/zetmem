{
  "mcpServers": {
    "zetmem-augmented": {
      "command": "/path/to/zetmem-server/zetmem-server",
      "args": ["-config", "/path/to/zetmem-server/config/production.yaml"],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "ZETMEM_ENV": "production",
        "ZETMEM_LOG_LEVEL": "info"
      }
    }
  }
}
